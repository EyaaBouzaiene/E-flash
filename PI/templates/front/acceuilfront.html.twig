 tension: 1,
                data: {{ qualite|raw }}
            }]
        },
        options: {
			 animations: {
      tension: {
        duration: 1000,
        easing: 'linear',
        from: 1,
        to: 0,
        loop: true
      }
    },
            scales: {
                yAxes: [{
                    ticks: {
                        beginAtZero: true

                    }
					
                }]
            }
        }
    })
</script>





